{
  "name": "rbal-luka-portal",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "check-formatting": "prettier -c src --end-of-line auto",
    "fix-formatting": "prettier -w src --end-of-line auto",
    "lint": "eslint src --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@rbal-modern-luka/ui-library": "0.0.0-main-0158a1db",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-focus-lock": "^2.9.1",
    "react-query": "^3.39.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-tooltip": "^5.27.1",
    "single-spa": "^5.9.4",
    "single-spa-react": "^4.6.1",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@rbal-modern-luka/eslint-codestyle": "0.0.0-main-6748acec",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^2.0.0",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.1",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "3.0.9"
  },
  "jest": {
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "sourcemap": true
        }
      ]
    }
  },
  "workspaces": [
    "packages/rbal-luka-portal-shell",
    "packages/rbal-luka-ufront-starter"
  ]
}
